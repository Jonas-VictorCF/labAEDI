#include <stdio.h>
#include <stdlib.h>



struct criar_carro
{
	char carro[30];

};
struct criar_nome
{
	char nome[30];
	
    
    struct criar_carro carro
    
}dono[3];




int main(int argc, char** argv)
{
	int i;
printf("Cadastro dos donos de cada carro \n\n");

for(i=0;i<3;i++){
	
	printf("nome do propietario do carro  ....:");
	fflush(stdin);
	gets(dono[i].nome);
	printf("O carro do propietario :");
	scanf("%s",&dono[i].carro.carro);
}
for(i=0;i<3;i++){
printf("\n Nome.... :%s\n",dono[i].nome);
printf("dono do carro.... :%s\n",&dono[i].carro.carro);
	
	
}
	