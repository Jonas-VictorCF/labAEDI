}D;




D* criaDono(char* nome, char* endereco, char* CPF){
 
 	D* res1 = (D*)malloc(sizeof(D))
    D -> C = C1;
	D -> C = endereco[61]; 
    D -> C = nome[51];
    D -> C = CPF[12];
	
	return(res1);
}


C* criaCarro(D* d1, char* placa, short ano, short multa){
	
  C* res = (C*)malloc(sizeof(C))	
	C-> D = d1 ;
	C-> D = carro;
	C-> D = *placa;
	C-> D = ano;
	C-> D = multa;
	
	return(res);
}

